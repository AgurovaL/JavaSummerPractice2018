<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
    <class name="com.agurova.models.Image" table="image">
        <id name="id" column="id" type="long">
            <generator class="identity" />
        </id>

        <property name="name" column="name"/>
        <property name="adress" column="address"/>

       <bag name="likedUsersList" table="favorite_image" cascade="all">
            <key column="id"></key>
            <many-to-many column="user_id" class="com.agurova.models.User"/>
        </bag>

    </class>
</hibernate-mapping>
